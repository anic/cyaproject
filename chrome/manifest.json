{
	"name": "My Office助手",
	"version": "0.1.1",

	"description": "用于改善各种系统的用户体验，减少复杂操作，提升工作效率。",
	"background_page": "background.html",
	"options_page": "feature.html",
	"permissions": ["tabs", "http://*/*", "notifications", "management"],
	"content_scripts": [
		{
			"matches": ["http://xqp2.gz.gmcc.net/*"],
			"all_frames": true,
			"js": ["js/inject_common.js", "js/inject_xqp_search.js"]
		}, {
			"matches": ["http://clgl.gz.gmcc.net/application/recordtaxiedit"],
			"js": ["js/inject_common.js", "js/inject_taxi.js"]
		}, {
			"matches": ["http://www.baidu.com/*"],
			"js": ["js/inject_common.js", "js/inject_baidu.js"]
		}, {
			"matches": ["http://contacts.services.gmcc.net/*"],
			"all_frames": true,
			"css": ["css/inject_albumn.css"]
		}, {
			"matches": ["http://portal.gmcc.net/portal/redirect.htm?*app"],
			"js": ["js/inject_common.js", "js/jquery.js", "js/inject_portal_login.js"]
		}, {
			"matches": ["http://pac.gmcc.net/*"],
			"all_frames": true,
			"run_at": "document_end",
			"js": ["js/jscssfile_loader.js", "js/inject_oa_middle.js"],
			"css": ["css/inject_oa_middle.css"]
		}, {
			"matches": ["http://pac.gmcc.net/", "http://pac.gmcc.net/index.html*"],
			"js": ["js/inject_pac_login.js"]
		}, {
			"matches": ["http://ad.services.gmcc.net/*"],
			"run_at": "document_end",
			"js": ["js/inject_oa_ad.js"]
		}
	],
	"icons": {
		"32": "image/home32.png",
		"48": "image/home48.png",
		"64": "image/home64.png"
	},
	"browser_action": {
		"default_icon": "image/home.png",
		"popup": "popup.html",
		"name": "My Office助手"
	}
}
